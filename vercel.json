{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*\\.js)",
      "dest": "/src/index.ts",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/api-docs/(.*\\.css)",
      "dest": "/src/index.ts",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
